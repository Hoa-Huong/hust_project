<tr class="user_<%= user.id %>">
  <td></td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.phone %></td>
  <td class="project-actions text-center">
    <%= link_to admin_user_path(user), class: "btn btn-primary btn-sm" do %>
      <i class="fas fa-folder"></i>
      <%= t"view" %>
    <% end %>
    <%= link_to admin_user_path(user), class: "btn btn-danger btn-sm", method: :delete, data: {confirm: t("confirm")} do %>
      <i class="fas fa-trash"></i>
      <%= t "del" %>
    <% end %>
  </td>
</tr>
